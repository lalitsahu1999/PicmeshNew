<resources xmlns:tools="http://schemas.android.com/tools">

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="DatePickerDialogTheme" parent="Theme.AppCompat.Light.Dialog">
        <item name="colorAccent">@color/date_picker_color</item>

    </style>

    <style name="draw_seekbar" parent="Widget.AppCompat.SeekBar">
        <item name="android:progressBackgroundTint">#A6FFFFFF</item>
        <item name="android:progressTint">#FFFFFF</item>
        <item name="thumbColor">#03F1AE</item>
        <item name="thumbElevation">5dp</item>
    </style>

</resources>
